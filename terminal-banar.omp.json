{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "background": "#0E050F",
            "foreground": "#EFDCF9",
            "style": "diamond",
            "template": " {{ .UserName }}",
            "type": "session"
          },
          {
            "background": "#170B3B",
            "foreground": "#EFDCF9",
            "powerline_symbol": "\ue0b4",
            "properties": {
              "folder_icon": "...",
              "folder_separator_icon": "/",
              "home_icon": "~",
              "max_depth": 3,
              "style": "agnoster_short"
            },
            "style": "powerline",
            "template": " {{ .Path }} ",
            "type": "path"
          },
          {
            "background": "#f54d27",
            "background_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#f54d27{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
              "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
              "{{ if gt .Behind 0 }}#c792ea{{ end }}"
            ],
            "foreground": "#f0efe7",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "branch_icon": "\ue725 ",
              "fetch_stash_count": true,
              "fetch_upstream_icon": true
            },
            "style": "powerline",
            "template": " {{ url .UpstreamIcon .UpstreamURL }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
            "type": "git"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "background": "#0087D8",
            "foreground": "#003544",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "properties": {
              "display_mode": "context",
              "fetch_virtual_env": true
            },
            "style": "powerline",
            "template": " \ue235 {{.Venv }}",
            "type": "python"
          },
          {
            "background": "#880000",
            "foreground": "#eae8fe",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "style": "powerline",
            "template": " \ue266 CPU: {{ round .PhysicalPercentUsed .Precision }}% ",
            "type": "sysinfo"
          },
          {
            "background": "#132644",
            "foreground": "#eae8fe",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "style": "powerline",
            "template": " \ue266 RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB ",
            "type": "sysinfo"
          },
          {
            "background": "#f36943",
            "background_templates": [
              "{{if lt .Percentage 15}}#e10615{{end}}",  
              "{{if and (gt .Percentage 15) (lt .Percentage 40)}}#ea5e12{{end}}",  
              "{{if and (gt .Percentage 40) (lt .Percentage 60)}}#f6a60b{{end}}",  
              "{{if and (gt .Percentage 60) (lt .Percentage 80)}}#fbdb15{{end}}",  
              "{{if and (gt .Percentage 80) (lt .Percentage 100)}}#93bf1e{{end}}", 
              "{{if gt .Percentage 100}}#24a434{{end}}", 
              "{{if eq \"Charging\" .State.String}}#0000FF{{end}}", 
              "{{if eq \"Discharging\" .State.String}}#008000{{end}}"
            ],
            "foreground": "#46434c",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "properties": {
              "charged_icon": "\uf240 ",
              "charging_icon": "\uf1e6 ",
              "discharging_icon": "\ue234 "
            },
            "style": "powerline",
            "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 ",
            "type": "battery"
          },
          {
            "background": "#003543",
            "foreground": "#fff",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "style": "powerline",
            "template": "<#fff> \ue641 </>{{ .CurrentDate | date \"15:04\" }} ",
            "type": "time"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "background": "#1DB954",
            "foreground": "#011627",
            "leading_diamond": "\ue0b6",
            "properties": {
              "playing_icon": "\uf1bc "
            },
            "style": "diamond",
            "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} ~ {{ .Track }}{{ end }} ",
            "trailing_diamond": "\ue0c6 ",
            "type": "spotify"
          },
          {
            "background": "#007800",
            "background_templates": [
              "{{ if gt .Code 0 }}#f1184c{{ end }}"
            ],
            "foreground": "#ffffff",
            "leading_diamond": "<transparent,background>\ue0b0</>",
            "properties": {
              "always_enabled": false
            },
            "style": "diamond",
            "template": " \uea6c ",
            "trailing_diamond": "\ue0b0",
            "type": "status"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#007ACC",
            "style": "plain",
            "template": "ðŸ“Ž ",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
